<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WeatherWise.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WeatherWise"
    xmlns:mylogin="clr-namespace:WeatherWise.Views"
    Shell.FlyoutBehavior="Disabled">

    <FlyoutItem Route="Login">
        <ShellContent  ContentTemplate="{DataTemplate mylogin:LoginPage}"></ShellContent>
    </FlyoutItem>
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
